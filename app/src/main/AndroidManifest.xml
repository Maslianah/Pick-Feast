            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CatererDisplay"
            android:screenOrientation="portrait" />

        <!-- <activity android:name=".rowListing" android:screenOrientation="portrait"/> -->
        <activity
            android:name=".Signup"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Prompt"
            android:screenOrientation="portrait" />
        <activity
            android:name=".login_signup_home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".orderDetails"
